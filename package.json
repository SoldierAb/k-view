{
  "name": "@cgj/k-view",
  "version": "0.1.4",
  "main": "./lib/kview/index.js",
  "scripts": {
    "test": "jest --coverage --watchAll",
    "dev": "webpack-dev-server --config=./build/webpack.dev.js --open --theme=default",
    "build": "webpack  --config=./build/webpack.pub.js",
    "analyz": "NODE_ENV=production npm_config_report=true npm run pub",
    "release": "standard-version",
    "lint": "eslint --ext .js,.vue src examples --fix",
    "commit": "git add . && git-cz"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHooks": {
    "pre-commit": "npm run lint && git add .",
    "commit-msg": "node scripts/verify-commit.js"
  },
  "email": "ab140140@163.com",
  "keywords": [
    "eleme",
    "vue",
    "components"
  ],
  "author": "cgj",
  "license": "ISC",
