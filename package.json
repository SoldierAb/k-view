    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-kimport": "^1.2.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-es2015": "^7.0.0-beta.3",
